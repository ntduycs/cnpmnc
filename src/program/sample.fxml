<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane fx:controller="program.AdjacencyMatrix" xmlns:fx="http://javafx.com/fxml" alignment="center" hgap="10" vgap="10">
    <children>
        <TextField
                fx:id="sizeField" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
        <Button fx:id="generateButton" GridPane.columnIndex="0" GridPane.rowIndex="2"
                text="Generate Matrix"
                onAction="#onButtonClickGenerate"/>
    </children>
</GridPane>